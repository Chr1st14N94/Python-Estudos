from utilidadescev1 import moeda as md
p = float(input("digite o preço R$: "))
print(f'A metade de {md.format(p)} é {md.metade(p, True)}')
print(f'O dobro de {md.format(p)} é {md.dobro(p, True)}')
print(f'Aumentando 10%, temos {md.aumentou(p,10, True)}')
print(f'Reduzindo 13%, temos {md.diminuir(p, 13, True)}')
